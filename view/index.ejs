                        <a class="nav-item nav-link" id="nav-last-tab" data-toggle="tab" href="#nav-last" role="tab" aria-controls="nav-contact" aria-selected="false">Politic</a>
                        <a class="nav-item nav-link" id="nav-Sports" data-toggle="tab" href="#nav-nav-Sport" role="tab" aria-controls="nav-contact" aria-selected="false">Sports</a>
                        <a class="nav-item nav-link" id="nav-technology" data-toggle="tab" href="#nav-techno" role="tab" aria-controls="nav-contact" aria-selected="false">Others</a>
                      </div>
                    </nav>
                    <!--End Nav Button  -->
                  </div>
                </div>
              </div>
              <div class="row">
                <div class="col-12">
                  <!-- Nav Card -->
                  <div class="tab-content" id="nav-tabContent">
                    <!-- card one -->
                    <%- include('partials/category_all', { news: allNews }) %>

                    <!-- Card two -->
                    <%- include('partials/category_education', { news: educationNews }) %>

                    <!-- Card three -->
                    <%- include('partials/category_business', { news: businessNews }) %>

                    <!-- card fure -->
                    <%- include('partials/category_politic', { news: politicsNews }) %>


                    <!-- card Five -->
                    <%- include('partials/category_sport', { news: sportsNews }) %>


                    <!-- card Six -->
                    <%- include('partials/category_others', { news: othersNews }) %>


                  </div>
                  <!-- End Nav Card -->
                </div>
              </div>
            </div>
            <div class="col-lg-4">
